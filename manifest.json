{
    "manifest_version": 3,
    "name": "noInsta",
    "version": "1.0",
    "description": "Blocks Instagram unless you've completed at least one LeetCode problem.",
    "permissions": ["scripting"],
    "host_permissions": [
      "https://alfa-leetcode-api.onrender.com/*"
    ],
    "content_scripts": [
      {
        "matches": ["*://www.instagram.com/*"],
        "js": ["Content.js"]
      }
    ]
}